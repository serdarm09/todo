{
  "version": 2,
  "builds": [
    {
      "src": "app/manage.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/app/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "app/vercel_app/wsgi.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "app.settings",
    "PYTHONPATH": "/vercel/path0/app"
  }
}